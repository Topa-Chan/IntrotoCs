import java.util.Scanner;

public class Quiz {

	private Question[] questions;
	private int i = 0;
	
	public Quiz() {
		questions = new Question[25];
	}

	public void Add(Question q) {
		questions[i] = q;
		i++;
	}
	
	private void displayQuiz() {
		for (int i = 0; i < questions.length; i++) {
			System.out.println(questions[i]);
		}
	}
	
}
