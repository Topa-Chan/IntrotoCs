
public class MyString {

	private String value;
	
	public MyString() {
		
	}

	public MyString(String value) {
		this.value = value;
	}
	
	public MyString(long[] value) {
		String temp = "";
		for (int i = 0; i < value.length; i++) {
			temp += (char) value[i];
		}
		this.value = temp;
	}
	
	@Override
	public String toString() {
		return this.value;
	}
	
}
